	"version": "0.2.0",
	"configurations": [
		{
			"name": "All Tests",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"cwd": "${workspaceFolder}",
			"program": "${workspaceFolder}/node_modules/mocha/bin/mocha",
			"args": [
				"-u",
				"bdd",
				"tests/???*.js",
				"--timeout",
				"0",
				"--slow",
				"20",
				"--colors",
			],
			"outputCapture": "std",
			"internalConsoleOptions": "openOnSessionStart",
		},
